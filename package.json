{
	"name": "balancete",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"check": "biome check --write .",
		"dev": "pnpm -r dev",
		"build": "pnpm -r build",
		"check-types": "pnpm -r check-types",
		"dev:native": "pnpm --filter native dev",
		"dev:web": "pnpm --filter web dev",
		"dev:server": "pnpm --filter @balancete/backend dev",
		"dev:setup": "pnpm --filter @balancete/backend dev:setup"
	},
	"dependencies": {},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0"
	},
	"packageManager": "pnpm@10.14.0"
}
